{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "default_formatter": {
            "format": "[%(levelname)s:%(asctime)s] %(message)s"
        },
        "error_formatter": {
            "format": "%(levelname)s: %(message)s"
        }
    },
    "handlers": {
        "stream_handler": {
            "class": "logging.StreamHandler",
            "formatter": "default_formatter"
        }

    },
    "loggers": {
        "module_logger": {
            "handlers": [
                "stream_handler"
            ],
            "level": "DEBUG",
            "propagate": true
        }
    }
}